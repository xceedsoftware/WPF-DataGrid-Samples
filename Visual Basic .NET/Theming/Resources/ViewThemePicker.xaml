                             Offset="0"/>

               <GradientStop Color="#FFF9BE3A"
                             Offset="1"/>

               <GradientStop Color="#FFDA9700"
                             Offset="0.671"/>

            </LinearGradientBrush>
         </Setter.Value>
      </Setter>

      <Setter Property="BorderBrush">
