   <!-- The style of the small tokens in the full theme list palette. -->
   <Style x:Key="rdMiniTokenStyle"
          TargetType="ContentPresenter">
      <Setter Property="Width"
              Value="10"/>
      <Setter Property="Height"
              Value="10"/>
   </Style>

   <!-- The text of the RadioButton in the full Theme list palette. -->
   <Style x:Key="rdButtonTextStyle"
          TargetType="TextBlock">
      <Setter Property="Foreground"
              Value="#FFDADADA"/>
      <Setter Property="TextWrapping"
              Value="Wrap"/>
      <Setter Property="FontWeight"
              Value="Normal"/>
      <Setter Property="Margin"
              Value="5,0,0,0"/>
      <Setter Property="VerticalAlignment"
              Value="Center"/>
   </Style>

