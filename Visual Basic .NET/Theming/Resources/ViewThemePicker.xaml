   <Style BasedOn="{x:Null}"
          TargetType="{x:Type Button}"
          x:Key="bPalette">

      <Setter Property="FocusVisualStyle"
              Value="{x:Null}"/>

      <Setter Property="Template">
         <Setter.Value>
            <ControlTemplate TargetType="{x:Type Button}">
               <ControlTemplate.Resources>
                  <Storyboard x:Key="OverPalette">
                     <ColorAnimationUsingKeyFrames BeginTime="00:00:00"
                                                   Storyboard.TargetName="txtTemplate"
                                                   Storyboard.TargetProperty="(TextElement.Foreground).(SolidColorBrush.Color)">
                        <SplineColorKeyFrame KeyTime="00:00:00.2000000"
                                             Value="#FFFFFFFF"/>
                     </ColorAnimationUsingKeyFrames>
                     <ColorAnimationUsingKeyFrames BeginTime="00:00:00"
                                                   Storyboard.TargetName="txtTheme"
                                                   Storyboard.TargetProperty="(TextElement.Foreground).(SolidColorBrush.Color)">
                        <SplineColorKeyFrame KeyTime="00:00:00.2000000"
                                             Value="#FFFFFFFF"/>
                     </ColorAnimationUsingKeyFrames>
                  </Storyboard>
               </ControlTemplate.Resources>

               <Grid HorizontalAlignment="Left"
                     x:Name="grid"
                     VerticalAlignment="Top">

                  <Grid HorizontalAlignment="Left"
                        VerticalAlignment="Bottom"
                        Width="175"
                        Height="40"
                        x:Name="LookPalette">
                     <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="90"/>
                        <ColumnDefinition Width="*"/>
                     </Grid.ColumnDefinitions>
                     <Border CornerRadius="8,0,8,8"
                             BorderThickness="1,1,2,3"
                             Margin="3,3,0,0"
                             Width="Auto"
                             Height="Auto"
                             Grid.ColumnSpan="2"
                             x:Name="borderBlack">
                        <Border.Background>
                           <LinearGradientBrush EndPoint="1,0.5"
                                                StartPoint="0,0.5">
                              <GradientStop Color="#FF777777"
                                            Offset="0"/>
                              <GradientStop Color="#FF212121"
                                            Offset="1"/>
                           </LinearGradientBrush>
                        </Border.Background>
                        <Border.BorderBrush>
                           <LinearGradientBrush EndPoint="1,0.5"
                                                StartPoint="0,0.5">
                              <GradientStop Color="#FFB9B9B9"
                                            Offset="0"/>
                              <GradientStop Color="#FF575757"
                                            Offset="1"/>
                           </LinearGradientBrush>
                        </Border.BorderBrush>
                     </Border>
                     <Border Margin="0,0,0,0"
                             Grid.Column="0"
                             CornerRadius="8,8,8,8"
                             BorderThickness="2,1,2,3"
                             x:Name="borderOrange">
                        <Border.BorderBrush>
                           <LinearGradientBrush EndPoint="1,0.5"
                                                StartPoint="0,0.5">
                              <GradientStop Color="#FFFBA900"
                                            Offset="0"/>
                              <GradientStop Color="#FFCC5300"
                                            Offset="1"/>
                           </LinearGradientBrush>
                        </Border.BorderBrush>
                        <Border.Background>
                           <LinearGradientBrush EndPoint="0.489,0.942"
                                                StartPoint="0.489,0.075">
                              <GradientStop Color="#FFF9BE3A"
                                            Offset="1"/>
                              <GradientStop Color="#FFE18200"
                                            Offset="0.419"/>
                              <GradientStop Color="#FFF9BE3A"
                                            Offset="0"/>
                           </LinearGradientBrush>
                        </Border.Background>
                     </Border>
                     <Border CornerRadius="6,6,6,6"
                             BorderThickness="0,0,0,0"
                             BorderBrush="{x:Null}"
                             Margin="2,2,3,0"
                             VerticalAlignment="Top"
                             Width="Auto"
                             Height="18"
                             Background="#4CFFFFFF"
                             x:Name="borderGlass"/>
                     <TextBlock HorizontalAlignment="Stretch"
                                Margin="0,0,0,0"
                                VerticalAlignment="Center"
                                Width="Auto"
                                Height="Auto"
                                Background="{x:Null}"
                                FontFamily="Tahoma"
                                FontSize="11"
                                FontWeight="Normal"
                                Text="Change UI Style"
                                TextWrapping="Wrap"
                                Focusable="False"
                                x:Name="textBox"
                                TextAlignment="Center"/>
                     <TextBlock Width="Auto"
                                Height="Auto"
                                FontFamily="Tahoma"
                                FontSize="10"
                                FontWeight="Normal"
                                Text="{Binding Source={x:Static Application.Current}, Path=ActualViewName}"
                                TextWrapping="Wrap"
                                Focusable="False"
                                HorizontalAlignment="Right"
                                Margin="0,0,5,-12"
                                x:Name="txtTemplate"
                                VerticalAlignment="Center"
                                FontStyle="Italic"
                                Foreground="#FFC9C9C9"
                                Grid.Column="1"/>
                     <TextBlock Width="Auto"
                                Height="Auto"
                                FontFamily="Tahoma"
                                FontSize="10"
                                FontWeight="Normal"
                                Text="{Binding Source={x:Static Application.Current}, Path=ActualThemeName}"
                                TextWrapping="Wrap"
                                FontStyle="Italic"
                                Foreground="#FFC9C9C9"
                                Focusable="False"
                                HorizontalAlignment="Right"
                                Margin="0,-12,5,0"
                                x:Name="txtTheme"
                                VerticalAlignment="Center"
                                Grid.Column="1"/>
                  </Grid>
               </Grid>

               <ControlTemplate.Triggers>
                  <Trigger Property="IsFocused"
                           Value="True">
                     <Setter Property="BorderBrush"
                             TargetName="borderOrange">
                        <Setter.Value>
                           <LinearGradientBrush EndPoint="1,0.5"
                                                StartPoint="0,0.5">
                              <GradientStop Color="#FFFBA900"
                                            Offset="0"/>
                              <GradientStop Color="#FFCC5300"
                                            Offset="1"/>
                           </LinearGradientBrush>
                        </Setter.Value>
                     </Setter>
                  </Trigger>
                  <Trigger Property="IsDefaulted"
                           Value="True">
                     <Setter Property="BorderBrush"
                             TargetName="borderOrange">
                        <Setter.Value>
                           <LinearGradientBrush EndPoint="1,0.5"
                                                StartPoint="0,0.5">
                              <GradientStop Color="#FFFBA900"
                                            Offset="0"/>
                              <GradientStop Color="#FFCC5300"
                                            Offset="1"/>
                           </LinearGradientBrush>
                        </Setter.Value>
                     </Setter>
                  </Trigger>
                  <Trigger Property="Tag"
                           Value="IsMouseOver">
                     <Trigger.EnterActions>
                        <BeginStoryboard x:Name="OverPalette_BeginStoryboard"
                                         Storyboard="{StaticResource OverPalette}"/>
                     </Trigger.EnterActions>
                     <Trigger.ExitActions>
                        <RemoveStoryboard BeginStoryboardName="OverPalette_BeginStoryboard"/>
                     </Trigger.ExitActions>
                     <Setter Property="BorderBrush"
                             TargetName="borderOrange">
                        <Setter.Value>
                           <LinearGradientBrush EndPoint="1,0.5"
                                                StartPoint="0,0.5">
                              <GradientStop Color="#FFFBA900"
                                            Offset="0"/>
                              <GradientStop Color="#FFCC5300"
                                            Offset="1"/>
                           </LinearGradientBrush>
                        </Setter.Value>
                     </Setter>
                  </Trigger>
                  <Trigger Property="IsPressed"
                           Value="True">
                     <Setter Property="BorderBrush"
                             TargetName="borderOrange">
                        <Setter.Value>
                           <LinearGradientBrush EndPoint="1,0.5"
                                                StartPoint="0,0.5">
                              <GradientStop Color="#FFFBA900"
                                            Offset="0"/>
                              <GradientStop Color="#FFCC5300"
                                            Offset="1"/>
                           </LinearGradientBrush>
                        </Setter.Value>
                     </Setter>
                  </Trigger>
                  <Trigger Property="IsEnabled"
                           Value="False">
                     <Setter Property="Background"
                             TargetName="borderOrange">
                        <Setter.Value>
                           <LinearGradientBrush EndPoint="0.489,0.942"
                                                StartPoint="0.489,0.075">
                              <GradientStop Color="#FF888783"
                                            Offset="1"/>
                              <GradientStop Color="#FF4A4A4A"
                                            Offset="0.419"/>
                              <GradientStop Color="#FF7B7B7B"
                                            Offset="0"/>
                           </LinearGradientBrush>
                        </Setter.Value>
                     </Setter>
                     <Setter Property="BorderBrush"
                             TargetName="borderOrange">
                        <Setter.Value>
                           <LinearGradientBrush EndPoint="1,0.5"
                                                StartPoint="0,0.5">
                              <GradientStop Color="#FF7B7B7B"
                                            Offset="0.005"/>
                              <GradientStop Color="#FF343434"
                                            Offset="1"/>
                           </LinearGradientBrush>
                        </Setter.Value>
                     </Setter>
                  </Trigger>
               </ControlTemplate.Triggers>
            </ControlTemplate>
         </Setter.Value>
      </Setter>
   </Style>

