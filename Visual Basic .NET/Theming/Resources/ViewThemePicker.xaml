                        Margin="0,0,0,0"
                        VerticalAlignment="Top"
                        Grid.Column="1"
                        Grid.Row="1">
                     <Path.Fill>
                        <RadialGradientBrush
                           Center="0.5,0.5"
                           GradientOrigin="0.154,0.388"
                           RadiusX="0.5"
                           RadiusY="0.5">
                           <RadialGradientBrush.RelativeTransform>
                              <TransformGroup>
                                 <ScaleTransform CenterX="0.5"
                                                 CenterY="0.5"
                                                 ScaleX="1.171"
                                                 ScaleY="3.305"/>
